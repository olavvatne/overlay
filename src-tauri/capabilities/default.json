{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "settings"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "dialog:default",
    "fs:allow-home-read",
